{
  "name": "Temporary Tab",
  "version": "0.1",
  "permissions": [
	  "tabs",
	  "storage",
	  "idle"
  ],
  "background": {
	  "scripts": [
		  "background.js"
	  ],
	  "persistent": false
  },
  "commands": {
    "temporaryTab": {
      "suggested_key": { 
		  "windows": "Ctrl+Shift+Y",
		  "mac": "Command+Shift+Y"
	  },
      "description": "Open a temporary tab"
    },
    "selfDestruct": {
      "suggested_key": { 
		  "windows": "Alt+Shift+Y",
		  "mac": "Alt+Shift+Y"
	  },
      "description": "Make the current tab self destruct"
    }
  },
  "manifest_version": 2
}
